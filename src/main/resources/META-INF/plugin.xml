<idea-plugin>
    <id>io.github.schorcher.stringInterpolation</id>
    <name>Java String Interpolation</name>
    <vendor>Schorcher</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <languageInjector implementation="io.github.schorcher.stringInterpolation.InterpolatedStringInjector" order="first"/>
    </extensions>
</idea-plugin>
